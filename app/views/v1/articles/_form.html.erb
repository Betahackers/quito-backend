<div class='row'>
  <div class='col-md-5'>
    <%= simple_form_for @article do |f| %>
      <%= f.input :title %>
      <%= f.input :content %>
      <%= f.input :mood_list, input_html: {class: 'select2_tags'} %>
      <%= f.input :category_list, input_html: {class: 'select2_tags'} %>
      <%= f.submit %>
    <% end %>
  </div>
</div>